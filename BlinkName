int led1 = D6; 

void setup() {


    pinMode(led1, OUTPUT);

}


void loop() {
    delay(2000);
    digitalWrite(led1, HIGH);
    delay(600);
    digitalWrite(led1, LOW);
    delay(200);
    digitalWrite(led1, HIGH);
    delay(200);
    digitalWrite(led1, LOW);
    delay(200);
    digitalWrite(led1, HIGH);
    delay(200);
    digitalWrite(led1, LOW);
    delay(200);
    digitalWrite(led1, HIGH);
    delay(200);
    digitalWrite(led1, LOW);
    delay(200);
    
